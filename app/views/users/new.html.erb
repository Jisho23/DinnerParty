<%if @user.errors.any? %>
  <ul>
    <%@user.errors.full_messages.each do |msg|%>
      <li><%=msg%></li>
    <%end%>
  </ul>
<%end%>
<h1> Create a New User</h1>
<%= form_for @user do |f| %>
 <%=f.label :username%>
 <%=f.text_field :username%>
 <%=f.label :password%>
 <%=f.password_field :password%>
 <%=f.label :name%>
 <%=f.text_field :name%>
 <%=f.submit 'Create User'%>
<%end%>
